{
  "expo": {
    "name": "Venn",
    "slug": "Venn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ios-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.venn.vennTakeHomeTest",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-foreground.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.venn.vennTakeHomeTest"
    },
    "plugins": [
      "expo-font",
      [
        "expo-splash-screen",
        {
          "ios": {
            "backgroundColor": "#ffffff",
            "image": "./assets/splash-icon.png",
            "resizeMode": "cover"
          },
          "android": {
            "backgroundColor": "#ffffff",
            "image": "./assets/splash-icon.png",
            "imageWidth": 150
          }
        }
      ]
    ]
  }
}
